

    <form action="/peep" method="post">
      Peep: <input type="text" name="peep">
      <input type="submit" value="Send peep!">
    </form>

    <h3> P E E P S: </h3> <% @peeps.reverse.each do |peep| %>
      <div>
     <%= peep.content %>, <%= peep.created_at.asctime %>
      </div>
    <% end %>
